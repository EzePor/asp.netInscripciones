<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Inscripciones</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Inscripciones.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-md navbar-toggleable-sm navbar-light bg-var(--tropical-indigoNav) border-bottom box-shadow">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Inscripciones</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item ">
                            <a class="navlink " asp-area="" asp-controller="Home" asp-action="Index">Inicio</a>
                        </li>
                        <li class="nav-item">
                            <a class="navlink " asp-area="" asp-controller="Home" asp-action="Nosotros">Nosotros</a>
                        </li>
                        <li class="nav-item">
                            <a class="navlink " asp-area="" asp-controller="Home" asp-action="aniversario35">Aniversario</a>
                        </li>
                        <li class="nav-item">
                            <a class="navlink " asp-area="" asp-controller="Alumnos" asp-action="Index">Alumnos</a>
                        </li>
                        <li class="nav-item">
                            <a class="navlink " asp-area="" asp-controller="Carreras" asp-action="Index">Carreras</a>
                        </li>
                        <li class="nav-item">
                            <a class="navlink " asp-area="" asp-controller="Inscripciones" asp-action="Index">Inscripciones</a>
                        </li>
                        <li class="nav-item">
                            <a class="navlink " asp-area="" asp-controller="AnioCarreras" asp-action="IndexAnioPorCarrera">Año Carrera</a>
                        </li>
                        <li class="nav-item">
                            <a class="navlink " asp-area="" asp-controller="Materias" asp-action="IndexPorAnio">Materias</a>
                        </li>
                        <li class="nav-item">
                            <a class="navlink " asp-area="" asp-controller="DetalleInscripciones" asp-action="IndexPorInscripcion">Detalle de Inscripciones</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - Inscripciones - &copy; Copyright - <a class="enlaceNew" href="https://ezepor.vercel.app/" target="_blank">Desarrollo y diseño EzePor</a>
        </div>
    </footer>
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
